{
  "openapi": "3.0.1",
  "info": {
    "title": "API External Account Inquiry",
    "description": "API External Account Inquiry",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "paths": {
    "/try-api/external-account-inquiry": {
      "post": {
        "operationId": "_post",
        "parameters": [
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-SIGNATURE",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-TIMESTAMP",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default": "2024-03-13T20:50:11+00:00"
            }
          },
          {
            "name": "X-PARTNER-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default": "889"
            }
          },
          {
            "name": "X-EXTERNAL-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "CHANNEL-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default":"12345"
            }
          }

        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "beneficiaryAccountNo",
                  "beneficiaryBankCode",
                  "partnerReferenceNo"
                ],
                "properties": {
                  "beneficiaryBankCode": {
                    "type": "string"
                  },
                  "beneficiaryAccountNo": {
                    "type": "string"
                  },
                  "partnerReferenceNo": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BsmCoreApi.resources.snap.inquiryOnlineTransfer:_post_$response"
                }
              }
            }
          },
          "401": {
            "description": "Access Denied",
            "content": {}
          }
        },
        "x-codegen-request-body-name": "additionalInfo"
      }
    }
  },
  "components": {
    "schemas": {
      "BsmCoreApi.resources.snap.inquiryOnlineTransfer:_post.additionalInfo": {
        "required": [
          "currencyDebit",
          "sourceAccount",
          "sourceAccountType",
          "transactionDatetime"
        ],
        "type": "object",
        "properties": {
          "currencyDebit": {
            "type": "string"
          },
          "transactionDatetime": {
            "type": "string"
          },
          "sourceAccount": {
            "type": "string"
          },
          "sourceAccountType": {
            "type": "string"
          }
        }
      },
      "BsmCoreApi.resources.snap.inquiryOnlineTransfer:_post_$response": {
        "required": [
          "beneficiaryAccountName",
          "beneficiaryAccountNo",
          "beneficiaryBankName",
          "responseCode",
          "responseMessage"
        ],
        "type": "object",
        "properties": {
          "beneficiaryAccountNo": {
            "type": "string"
          },
          "beneficiaryAccountName": {
            "type": "string"
          },
          "beneficiaryBankName": {
            "type": "string"
          },
          "responseMessage": {
            "type": "string"
          },
          "responseCode": {
            "type": "string"
          }
        }
      }
    }
  },
