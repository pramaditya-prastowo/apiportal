{
  "openapi": "3.0.1",
  "info": {
    "title": "registration",
    "description": "Registration for BSM Direct Debit API",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "paths": {
    "/try-api/registration": {
      "post": {
        "operationId": "_post",
        "parameters": [
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-SIGNATURE",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-TIMESTAMP",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default": "2024-03-13T20:50:11+00:00"
            }
          },
          {
            "name": "X-PARTNER-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default": "889"
            }
          },
          {
            "name": "X-EXTERNAL-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "CHANNEL-ID",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "default":"12345"
            }
          }

        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "bankCardNo",
                  "custIdMerchant",
                  "expiryDate",
                  "partnerReferenceNo",
                  "phoneNo"
                ],
                "properties": {
                  "partnerReferenceNo": {
                    "type": "string",
                    "description": ""
                  },
                  "bankCardNo": {
                    "type": "string",
                    "description": ""
                  },
                  "expiryDate": {
                    "type": "string",
                    "description": ""
                  },
                  "custIdMerchant": {
                    "type": "string",
                    "description": ""
                  },
                  "phoneNo": {
                    "type": "string",
                    "description": ""
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BsmDirectDebitApiSnap.resources.snap.registration:_post_$response"
                }
              }
            }
          },
          "401": {
            "description": "Access Denied",
            "content": {}
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BsmDirectDebitApiSnap.resources.snap.registration:_post_$response": {
        "required": [
          "additionalInfo",
          "bankCardToken",
          "chargeToken",
          "responseCode",
          "responseMessage"
        ],
        "type": "object",
        "properties": {
          "bankCardToken": {
            "type": "string"
          },
          "additionalInfo": {
            "$ref": "#/components/schemas/BsmDirectDebitApiSnap.resources.snap.registration:_post.additionalInfo"
          },
          "responseMessage": {
            "type": "string"
          },
          "responseCode": {
            "type": "string"
          },
          "chargeToken": {
            "type": "string"
          }
        }
      },
      "BsmDirectDebitApiSnap.resources.snap.registration:_post.additionalInfo": {
        "required": [
          "phoneNo"
        ],
        "type": "object",
        "properties": {
          "phoneNo": {
            "type": "string"
          }
        }
      }
    }
  },
